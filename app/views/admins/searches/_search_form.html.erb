<%= form_with(url: admins_search_path, method: :get, local: true) do |f| %>
  <span class="glyphicon glyphicon-search"></span>
  <%= text_field_tag 'search[content]' %><br>
  <%= select_tag 'search[model]', options_for_select({"Product": "product","Customer": "customer"}) %>
  <%= select_tag 'search[how]', options_for_select({"前方一致": "forward","完全一致": "match", "後方一致": "backward", "部分一致": "partical"}) %>
  <%= f.submit "この条件で探す！" %>
<% end %>