<%= form_with(url: admins_search_path, method: :get, local: true) do |f| %>
  <%= f.label :検索 %>
  <%= text_field_tag 'search[content]' %><br>
  <%= select_tag 'search[model]', options_for_select({"Customer": "customer", "Product": "product"}) %>
  <%= select_tag 'search[how]', options_for_select({"完全一致": "match", "前方一致": "forward", "後方一致": "backward", "部分一致": "partical"}) %>
  <%= f.submit %>
<% end %>